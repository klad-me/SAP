/**
 * Интерфейс Compareable предназначен для механизма сравнения двух объектов.<br/>
 * Интерфейс состоит из единственного метода - equals.
 */
interface Compareable
{
    /**
     * Сравнить вызываемый объект с указанным объектом.
     *
     * @param o объект, с которым необходимо сравнить вызываемый объект
     * @return true, в случае, если объекты равны, или false в остальных случаях
     */
    boolean equals(Object o);
}
