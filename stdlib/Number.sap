/**
 * Определяет интерфейс для получения числового значения класса
 */
interface Number
{
    /**
     * Получить значение в целом виде
     *
     * @return значение, преведеное к типу int
     */
    int intValue();
    
    /**
     * Получить значение в вещественном виде
     *
     * @return значение, приведенное к типу float
     */
    float floatValue();
}
